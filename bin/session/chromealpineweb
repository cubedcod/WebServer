#!/bin/sh

# vbutil_kernel --pack kern --keyblock /usr/share/vboot/devkeys/kernel.keyblock --version 1 --signprivate /usr/share/vboot/devkeys/kernel_data_key.vbprivk --config=.kargs --vmlinuz ../../linux/arch/x86_64/boot/bzImage --bootloader .bootstub
# dd if=kern of=/dev/nvme0n1p2
# cgpt add -i 2 -P 2 -S 0 -T 1 /dev/nvme0n1

sudo mount -o bind /dev dev
sudo mount -o bind /dev/pts dev/pts
sudo mount -o bind /dev/shm dev/shm
sudo mount -o bind /proc proc
sudo mount -o bind /sys sys
sudo mount -o bind /run run

sudo chroot . /bin/su -lc web_console user
