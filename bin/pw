#!/bin/sh
cd $(realpath ~/web)
app=$(realpath ~/src/pw)
uid=$(id -u)
su -c "sh $app/sh/network $uid $app/sh"
squid -f .conf/routeHTTP.conf
tmux new-session -s HTTP -d
tmux send-keys -t HTTP:0 'unicorn -N -l 127.0.0.1:8000 -l [::1]:8000 .conf/rack.ru' C-m
tmux new-window -t HTTP:1 -d $app/ruby/log_view
tmux attach-session -t HTTP
